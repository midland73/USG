program Project1;

uses
  Vcl.Forms,
  Zastavka in 'Zastavka.pas' {Zastavka},
  MainMenu in 'MainMenu.pas' {MainMenu},
  LoadingR in 'LoadingR.pas' {lLoadingR},
  LoadingL in 'LoadingL.pas' {lLoadingL},
  Unit5 in 'Unit5.pas' {Form5},
  Unit6 in 'Unit6.pas' {Zhiliber},
  Unit7 in 'Unit7.pas' {Dialogue},
  Unit8 in 'Unit8.pas' {Frog},
  Unit9 in 'Unit9.pas' {Mail},
  Unit10 in 'Unit10.pas' {Clown},
  Unit11 in 'Unit11.pas' {Loyalty},
  Unit12 in 'Unit12.pas' {Lighthouse},
  Unit13 in 'Unit13.pas' {Rook},
  Unit14 in 'Unit14.pas' {Worker},
  Unit15 in 'Unit15.pas' {Grunwald},
  Test in 'Test.pas' {tTest};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TzZastavka, zZastavka);
  Application.CreateForm(TForm5, Form5);
  Application.CreateForm(TtTest, tTest);
  Application.CreateForm(TGrunwald, Grunwald);
  Application.CreateForm(TRook, Rook);
  Application.CreateForm(TLighthouse, Lighthouse);
  Application.CreateForm(TLoyalty, Loyalty);
  Application.CreateForm(TClown, Clown);
  Application.CreateForm(TMail, Mail);
  Application.CreateForm(TFrog, Frog);
  Application.CreateForm(TZhiliber, Zhiliber);
  Application.CreateForm(TDialogue, Dialogue);
  Application.CreateForm(TmMainMenu, mMainMenu);
  Application.CreateForm(TlLoadingR, lLoadingR);
  Application.CreateForm(TlLoadingL, lLoadingL);
  Application.CreateForm(TWorker, Worker);
  Application.Run;
end.

