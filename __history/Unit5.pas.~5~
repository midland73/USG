uses Unit4, Unit6;

procedure TForm5.BitBtn1Click(Sender: TObject);
begin
Form4.Show;
Form5.Hide;
end;

procedure TForm5.Image2Click(Sender: TObject);
begin
 if Label2.Visible then
    Label2.Visible := False
  else
    Label2.Visible := True;
end;



